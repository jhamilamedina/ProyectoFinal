						logo BLOB,
						nombre TEXT,
						sede_principal TEXT,
						descripcion TEXT,
						sitio_web TEXT,
						fecha_creacion datetime,
						fecha_actualizacion datetime);
						
						CREATE TABLE estrellas(estrella_id INTEGER PRIMARY KEY AUTOINCREMENT,
												empresa_id INTEGER,
												estrella_1 INTEGER,
												estrella_2 INTEGER,
												estrella_3 INTEGER,
												estrella_4 INTEGER,
												estrella_5 INTEGER,
												FOREIGN KEY(empresa_id)REFERENCES empresas(empresa_id));
												
												CREATE TABLE valoracion(valoracion_id INTEGER PRIMARY KEY AUTOINCREMENT,
												empresa_id INTEGER,
												puntualidad INTEGER,
												seguridad INTEGER,
												economica INTEGER,
												amabilidad INTEGER,
												caro INTEGER,
												inseguro INTEGER,
												impuntual INTEGER,
												FOREIGN KEY(empresa_id)REFERENCES empresas(empresa_id));
												
												CREATE TABLE comentarios(comentario_id INTEGER PRIMARY KEY AUTOINCREMENT,
												fecha datetime,
												comentario TEXT,
												user_id INTEGER,
												empresa_id INTEGER,
												FOREIGN KEY(user_id)REFERENCES usuario(user_id),
												FOREIGN KEY(empresa_id)REFERENCES empresas(empresa_id));
												
												CREATE TABLE agencias_lima(agenciaslima_id INTEGER PRIMARY KEY AUTOINCREMENT,
												empresa_id INTEGER,
												foto BLOB,
												direccion TEXT,
												horario_de_atencion TEXT,
												telefono INTEGER,
												cochera INTEGER,
												salidas TEXT,
												llegadas TEXT,
												FOREIGN KEY(empresa_id)REFERENCES empresas(empresa_id));
												
												CREATE TABLE distritos(distrito_id INTEGER PRIMARY KEY AUTOINCREMENT,
																		agenciaslima_id INTEGER,
																		provincia_id INTEGER,
																		nombre TEXT,
																		FOREIGN KEY(agenciaslima_id)REFERENCES agencias_lima(agenciaslima_id),
																		FOREIGN KEY(provincia_id)REFERENCES provincia(provincia_id));
																		
